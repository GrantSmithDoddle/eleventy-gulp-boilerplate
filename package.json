{
  "name": "eleventy-gulp-boilerplate",
  "version": "1.0.0",
  "description": "Build sites with this Eleventy and Gulp Boilerplate",
  "main": "eleventy.config.js",
  "scripts": {
    "watch": "ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve & npx gulp",
    "debug": "DEBUG=Eleventy* ELEVENTY_ENV=development npx gulp && npx @11ty/eleventy --config=eleventy.config.js --serve",
    "optimise": "npx gulp images",
    "production": "NODE_ENV=production npx gulp production && ELEVENTY_ENV=production npx @11ty/eleventy --config=eleventy.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:granttransition/eleventy-gulp-boilerplate.git"
  },
  "author": "Grant Smith",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-image": "^6.2.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.4.0",
    "gulp-webp": "^4.0.1",
    "postcss": "^8.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "@quasibit/eleventy-plugin-sitemap": "^2.0.2"
  }
}
